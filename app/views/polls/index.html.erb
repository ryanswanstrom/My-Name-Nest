<% title "Polls" %>

<h1 class="boy_title">Welcome</h1>
<table>
  <tr>
    <th>Title</th>
  </tr>
  <% for poll in @polls %>
    <tr>
      <td><%=h poll.title %></td>
      <td><%= link_to "Show", poll_path(poll) %></td>
      <td><%= link_to "Edit", edit_poll_path(poll) %></td>
      <td><%= link_to "Destroy", poll, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Poll", new_poll_path %></p>
